{
  "name": "Simply Bookmark Manager",
  "version": "1",
  "description": "Open a new page with a simple bookmark manager.",
  "permissions": ["tabs", "bookmarks"],
  "browser_action":
  {
    "default_icon": "open.png"
  },
  "manifest_version": 2,"icons":{
    "16": "16.png",
    "48": "48.png",
    "128": "128.png"
  },
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'",
  "background":{
  "scripts": ["background.js"]
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  }
}
